string = input ()

if len(string)==11:
  a = int(input())
  b = int(input())
  c = int(input())
  p = ((a + b + c) / 2)
  print ((p*(p-a)*(p-b)*(p-c))**0.5)
  
elif len(string)==13:
  a = int(input())
  b = int(input())
  print (a*b)
  
elif len(string)==4:
  pi = 3.14
  r = int(input())
  print (pi*r**2)
